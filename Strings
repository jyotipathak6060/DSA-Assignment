#include <iostream>
#include <string>
using namespace std;
int main() {
    string a;
    string b;
    cin>>a>>b;
    int l1 = a.size();
    int l2 = b.size();
    string c= a+b;
    char x=a[0];
    a[0]=b[0];
    b[0]=x;
    cout<<l1<<" "<<l2\n;
    cout<<c\n;
    cout<<a<<" "<<b;
  
    return 0;
}
